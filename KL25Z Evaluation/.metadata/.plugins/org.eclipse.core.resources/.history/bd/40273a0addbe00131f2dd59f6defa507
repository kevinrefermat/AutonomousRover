#include <stdio.h>
#include "derivative.h" /* include peripheral declarations */

void delay( int value )
{
	int i;
	for ( i = 0; i < value; i++ );
}

int main(void)
{
	int counter, i;
	
	for( counter = 0; counter < 1000; counter++ )
	{
		i += 2;
		i *= 3;
		i -= 4;
	}
	printf( "done %d \n", i );
	for( ; ; );
	
	return 0;
}
